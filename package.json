{
  "name": "@jalexw/mould",
  "private": false,
  "description": "Generate sample projects and insert code snippets from your configurable templates collection",
  "version": "0.0.3",
  "dependencies": {
    "commander": "14.0.0",
    "zod": "3.23.8"
  },
  "devDependencies": {
    "typescript": "5.9.2",
    "bun-types": "1.2.21",
    "tsc-alias": "1.8.16"
  },
  "packageManager": "bun@1.2.21",
  "scripts": {
    "dev": "NODE_ENV=development bun run src/index.ts",
    "build:typescript": "tsc --project tsconfig.json && tsc-alias --project tsconfig.json",
    "build:executable": "mkdir -p ./dist/bin && NODE_ENV=production bun build ./src/index.ts --compile --outfile ./dist/bin/mould",
    "build": "bun run build:typescript && bun run build:executable",
    "postbuild": "bun run cleanup",
    "cleanup:compiled_tests_in_dist_directory": "find ./dist -type f \\( -name \"*.test.js\" -o -name \"*.test.js.map\" -o -name \"*.test.d.ts\" \\) -delete",
    "cleanup": "bun run cleanup:compiled_tests_in_dist_directory",
    "execute": "NODE_ENV=production bun run dist/index.js",
    "test": "bun test && rm -rf ./tmp"
  },
  "main": "dist/index.js",
  "type": "module",
  "types": "dist/index.d.ts",
  "module": "dist/index.js",
  "publishConfig": {
    "access": "public"
  }
}
