{
  "name": "@jalexw/mould",
  "description": "Generate sample projects and insert code snippets from your configurable templates collection",
  "version": "0.0.1",
  "dependencies": {
    "commander": "14.0.0",
    "zod": "3.23.8"
  },
  "devDependencies": {
    "typescript": "5.9.2",
    "bun-types": "1.2.21",
    "tsc-alias": "1.8.16"
  },
  "packageManager": "bun@1.2.21",
  "scripts": {
    "dev": "NODE_ENV=development bun run src/index.ts",
    "build:typescript": "tsc --project tsconfig.json && tsc-alias --project tsconfig.json",
    "build:executable": "mkdir -p ./dist/bin && NODE_ENV=production bun build ./src/index.ts --compile --outfile ./dist/bin/mould",
    "build": "bun run build:typescript && bun run build:executable",
    "execute": "NODE_ENV=production bun run dist/index.js",
    "test": "bun test"
  }
}
